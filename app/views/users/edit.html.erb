<h2>プロフィール編集</h2>
<%= form_with model: @user, local:true do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>

 <label for="inputold">年齢</label>
  <%= f.text_field :old, autofocus: true, id:"inputOld", placeholder:"年齢"%>

 <label for="inputName">身長</label>
  <%= f.text_field :height, autofocus: true, id:"inputHeight", placeholder:"身長"%>

<label for="inputName">体重</label>
  <%= f.text_field :body_weight, autofocus: true, id:"inputBodyWeight", placeholder:"体重"%>

  <label for="inputName">トレーニング歴</label>
  <%= f.text_field :experience, autofocus: true, id:"inputExperience", placeholder:"トレーニング歴"%>



   <%= attachment_image_tag @user, :profile_image, :fill, 60, 60%>
  <label for="inputImage">ProfileImage</label>
  <%= f.attachment_field :profile_image, placeholder: "プロフィール画像" %>

  <%= f.submit "変更を保存" %>
<% end %>