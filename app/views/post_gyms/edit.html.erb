<h1>編集画面</h1>
<!-- form_with部分 -->
<%= form_with model: @post_gym,url: post_gyms_path, method: :pacth, local: true do |f| %>
 <h4>today's best photo</h4>
  <%= f.attachment_field :image %>
  <h4>トレーニング場所</h4>
  <%= f.text_field :workout_at %>
  <h4>トレーニングメニュー</h4>
  <%= f.text_area :workout_menu %>
  <h4>トレーニング説明</h4>
  <%= f.text_area :aim %>
  <%= f.submit '保存' %>
<% end %>